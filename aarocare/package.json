    "@tiptap/react": "^2.14.0",
    "@tiptap/starter-kit": "^2.14.0",
    "aws-amplify": "^6.15.1",
    "axios": "^1.10.0",
    "clsx": "^1.2.1",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-datepicker": "^4.25.0",
